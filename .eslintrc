{
  "extends": [
    "standard",
    "standard-jsx",
    "standard-react",
    "plugin:prettier/recommended",
    "prettier/react",
    "prettier/standard",
    "plugin:jsx-a11y/recommended"
  ],
  "plugins": ["jsx-a11y"],
  "env": {
    "browser": true,
    "es6": true,
    "jest": true,
    "node": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2018,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "globals": {
    "cordova": true,
    "StatusBar": true
  }
}
